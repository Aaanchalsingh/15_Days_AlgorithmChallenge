class Solution {
    public boolean checkStraightLine(int[][] a) {
        //TWO POINT FORM
        for(int i=2;i<a.length;i++)
            if((a[1][1]-(a[0][1])*(a[i][0]-a[0][0])!=(a[1][0]-a[0][0])*(a[i][1]-a[0][1])) return false;
            // (y-y1)=m(x-x1)
            // (y-y1)=((y2-y1)/(x2-x1))(x-x1)
    return true;
    }
}
